    .data
prompt_L: .asciiz "Enter a number (L > 20): "
prompt_M: .asciiz "Enter a number (M > 20): "
prompt_N: .asciiz "Enter a number (N > 20): "
error:    .asciiz "Illegal Number!\n"
result:   .asciiz "Greatest Common Divisor: "
newline:  .asciiz "\n"

    .text
    .globl main

main:
    # Prompt user for input: L
    li $v0, 4
    la $a0, prompt_L
    syscall

input_L:
    li $v0, 5
    syscall
    move $t0, $v0
